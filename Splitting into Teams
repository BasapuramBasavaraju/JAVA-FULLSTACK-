import java.util.*;
public class Split
{
    public static void main(String args[])
    {
       Scanner Sc=new Scanner(System.in);
       int a=Sc.nextInt();
       int b=Sc.nextInt();
       int c=a/b;
       int d=a-b*c;
       System.out.printf("The number of students in each team is %d and left out is %d",c,d);
    }
}
