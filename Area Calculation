import java.util.*;
public class Area
{
    public static void main(String args[])
    {
      Scanner Sc=new Scanner(System.in);
      int a,b,y,z,d;
      float r;
      a=Sc.nextInt();
      b=Sc.nextInt();
      y=Sc.nextInt();
      r=Sc.nextFloat();
      z=a*a;
      d=b*y;
      System.out.println(z);
      System.out.println(d);
      System.out.printf("%.2f",(3.141)*r*r);
      
    }
}
