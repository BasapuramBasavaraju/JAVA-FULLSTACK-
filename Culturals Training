import java.util.*;
public class Football
{
    public static void main(String args[])
    {
        Scanner Sc=new Scanner(System.in);
        int a=Sc.nextInt();
        int b=Sc.nextInt();
        int c=a/b;
        int d=a%b;
        System.out.println(c);
        System.out.println(d);
    }
}
