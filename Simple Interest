import java.util.*;
public class Simple
{
    public static void main(String args[])
    {
        Scanner Sc=new Scanner(System.in);
        int a=Sc.nextInt();
        int b=Sc.nextInt();
        float c=Sc.nextFloat();
        float d=(a*b*c)/100;
        System.out.printf("%.2f",d);
        
    }
}
