import java.util.*;
public class SumDiff
{
    public static void main(String args[])
    {
        Scanner Sc=new Scanner(System.in);
        int a,b,c,d;
        a=Sc.nextInt();
        b=Sc.nextInt();
        c=a+b;
        d=a-b;
        System.out.println(c);
        System.out.println(d);
    }
}
