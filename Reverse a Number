import java.util.*;
public class Reverse
{
    public static void main(String[] args)
    {
        Scanner Sc=new Scanner(System.in);
        int a=Sc.nextInt();
        int b,sum=0;
        for(int i=0;i<=a;i++)
        {
            b=a%10;
            sum=(sum*10)+b;
            a=a/10;
        }
        System.out.println(""+sum+""+a);
        
    }
}
