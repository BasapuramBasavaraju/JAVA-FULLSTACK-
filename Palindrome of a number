import java.util.*;
public class Palindrome
{
    public static void main(String[] args)
    {
        Scanner Sc=new Scanner(System.in);
        int a=Sc.nextInt();
        int b,c=0,d=a;
        while(a>0)
        {
            b=a%10;
            c=(c*10)+b;
            a=a/10;
        }
            if(c==d)
            {
             System.out.println("Palindrome");
            }
            else
            {
                System.out.println(" Not a Palindrome");
            }
    }
}
