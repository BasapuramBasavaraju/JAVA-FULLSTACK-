import java.util.*;
public class ProDiv
{
    public static void main(String args[])
    {
        Scanner Sc=new Scanner(System.in);
        int a,b;
        a=Sc.nextInt();
        b=Sc.nextInt();
        System.out.println(a*b);
        System.out.println(a/b);
        System.out.println(a%b);
    }
}
