import java.util.*;
public class Flash
{
    public static void main(String args[])
       {
           Scanner Sc=new Scanner(System.in);
           int a=Sc.nextInt();
            int b=Sc.nextInt();
             int c=Sc.nextInt();
              int d=Sc.nextInt();
              float e=(a+c)/2;
              float f=b+d;
              System.out.println(e);
              System.out.println(f/2);
           
       }
    
}
