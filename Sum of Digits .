import java.util.*;
public class Sum
{
    public static void main(String[] args)
    {
        Scanner Sc=new Scanner(System.in);
        int a=Sc.nextInt();
        int sum,c=0;
        while(a>0)
        {
           sum=a%10;
           a=a/10;
           c=c+sum;
        }
        System.out.println(c);
        
    }
}
