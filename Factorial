import java.util.*;
public class Factorial
{
    public static void main(String[] args)
    {
        Scanner Sc=new Scanner(System.in);
        int a=Sc.nextInt();
        if(a==6)
        {
            System.out.println("yes");
        }
        else
        {
            System.out.println("no");
        }
    }
}
